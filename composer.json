{
    "name": "venveo/rssclient",
    "description": "Just a simple RSS client for Craft 3",
    "type": "craft-plugin",
    "version": "1.0.0-p1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "rssclient"
    ],
    "support": {
        "docs": "https://github.com/venveo/craft3-rssclient/blob/master/README.md",
        "issues": "https://github.com/venveo/craft3-rssclient/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Venveo",
            "homepage": "https://venveo.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "psr-4": {
          "venveo\\rssclient\\": "src/"
        }
    },
    "extra": {
        "name": "RSSClient",
        "handle": "rssClient",
        "schemaVersion": "1.0.0",
        "hasSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/venveo/craft3-rssclient/master/CHANGELOG.md",
        "components": {
            "rssFeedService": "venveo\\rssclient\\services\\RSSFeedService"
        },
        "class": "venveo\\rssclient\\RSSClient"
    }
}
